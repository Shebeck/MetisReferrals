<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    android:orientation="vertical"
    android:background="#ffffff">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/homeinfo"
        android:text="Profile:"
        android:textSize="25dp"
        android:textColor="#1a385a"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Name:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView4"
        android:layout_marginTop="16dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Name:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:layout_marginTop="35dp"
        android:id="@+id/textView9" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Middle initial:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView3"
        android:layout_marginStart="38dp"
        android:layout_marginTop="9dp"
    />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView8"
        android:layout_marginTop="15dp" />
    <!-- android:id="@+id/homename" -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView6"
        android:layout_marginStart="38dp"
        android:layout_marginTop="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="State:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView7"
        android:layout_marginTop="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zip:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView2"
        android:layout_marginTop="8dp" />
    <!-- android:id="@+id/homename" -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone Number:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView"
        android:layout_marginTop="7dp" />
    <!-- android:id="@+id/homename" -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:id="@+id/textView5"
        android:layout_marginTop="15dp" />
    <!-- android:id="@+id/homename" -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insurance:"
        android:textColor="#1a385a"
        android:textSize="15dp"
        android:layout_marginTop="7dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="90sp"
        android:layout_height="90sp"
        app:srcCompat="@drawable/bottomhomebuttonmdpi"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="478dp"
        android:background="@null"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="90sp"
        android:layout_height="90sp"
        app:srcCompat="@drawable/bottomdocsbuttonhdpi"
        tools:layout_editor_absoluteY="478dp"
        tools:layout_editor_absoluteX="93dp"
        android:background="@null"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageButton5" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="90sp"
        android:layout_height="90sp"
        app:srcCompat="@drawable/bottomapptsbuttonmdpi"
        android:background="@null"
        tools:layout_editor_absoluteY="478dp"
        tools:layout_editor_absoluteX="183dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageButton6" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="90sp"
        android:layout_height="90sp"
        app:srcCompat="@drawable/altbottomprofldpi"
        tools:layout_editor_absoluteX="278sp"
        tools:layout_editor_absoluteY="478sp"
        android:background="@null"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>